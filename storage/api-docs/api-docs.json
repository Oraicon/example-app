{
    "openapi": "3.0.0",
    "info": {
        "title": "Marketplace Back End Swager API Documentation",
        "description": "Marketplace Back End Swager API Description",
        "contact": {
            "email": "example@google.com"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "https://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0.0",
        "x-logo": {
            "url": "https://via.placeholder.com/190x90.png?text=MarketPlace"
        }
    },
    "servers": [
        {
            "url": "http://localhost:8000",
            "description": "Demo API Server"
        }
    ],
    "paths": {
        "/api/v1/product/{pageIndex}/{pageSize}": {
            "get": {
                "tags": [
                    "product"
                ],
                "summary": "Returns Product API response with Pagination",
                "description": "returns product API response with Pagination",
                "operationId": "product",
                "parameters": [
                    {
                        "name": "pageIndex",
                        "in": "query",
                        "description": "Page Index",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "1"
                    },
                    {
                        "name": "pageSize",
                        "in": "query",
                        "description": "Page Size",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "10"
                    },
                    {
                        "name": "filterBy",
                        "in": "query",
                        "description": "Filter By Available Columns",
                        "required": false,
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string",
                                "default": "product_name"
                            }
                        },
                        "example": "string 1,string 2,string 3"
                    },
                    {
                        "name": "sortBy",
                        "in": "query",
                        "description": "Sort By Descending",
                        "required": false,
                        "schema": {
                            "type": "boolean"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Operation"
                    },
                    "400": {
                        "description": "Bad Request"
                    },
                    "404": {
                        "description": "Not Found"
                    },
                    "500": {
                        "description": "Server Error"
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "api_key": {
                "type": "apiKey",
                "name": "api_key",
                "in": "header"
            },
            "petstore_auth": {
                "type": "oauth2",
                "flows": {
                    "implicit": {
                        "authorizationUrl": "https://petstore.swagger.io/oauth/dialog",
                        "scopes": {
                            "read:pets": "read your API",
                            "write:pets": "modify pets in your account"
                        }
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "product",
            "description": "Everything about your products",
            "externalDocs": {
                "description": "Find out more",
                "url": "https://swagger.io"
            }
        }
    ]
}